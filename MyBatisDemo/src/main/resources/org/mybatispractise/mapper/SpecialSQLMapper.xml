<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.mybatispractise.mapper.SpecialSQLMapper">
    <delete id="deleteMore">
        delete from user where username in (#{usernames})
    </delete>

    <select id="queryUserByLike" resultType="user">
        select * from user where username like concat('%',#{username},'%')
    </select>
    <select id="queryTest" resultType="org.mybatispractise.pojo.User">
        select * from user where age>#{age}
    </select>

    <!--通过自增的主键添加-->
    <insert id="insertStudentByGeneratedKey" useGeneratedKeys="true" keyProperty="id">
        insert into student values (null,#{name},#{age});
    </insert>
</mapper>
